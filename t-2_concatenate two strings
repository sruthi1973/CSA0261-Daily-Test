#include <stdio.h>
#include <string.h>

int main() {
    char str1[50], str2[50], merged[100];

    printf("Enter the first string: ");
    scanf("%s", str1);

    printf("Enter the second string: ");
    scanf("%s", str2);

    
    strcpy(merged, str1);
    strcat(merged, str2);

    printf("Merged string: %s\n", merged);

    return 0;
}
