#include<stdio.h>
void swap(int *n1,int *n2);
int main()
{int n1,n2,temp=0;
printf("Before swapping\n");
printf("enter n1: ");
scanf("%d",&n1);
printf("enter n2: ");
scanf("%d",&n2);
swap(&n1,&n2);
printf("After swapping\n");
printf("n1=%d\n",n1);
printf("n2=%d",n2);
return 0;
}

void swap(int *n1,int*n2)
{ 
  int temp;
  temp=*n1;
  *n1=*n2;
  *n2=temp;
}
